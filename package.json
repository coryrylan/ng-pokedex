{
  "name": "ng-pokedex",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "lint": "sass-lint && ng lint",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build": "ng build",
    "build--prod": "npm run lint && ng build --prod --build-optimizer --stats-json && npm run sw",
    "sw": "node workbox-build.js",
    "deploy": "npm run build--prod && firebase deploy",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.0.0-beta.7",
    "@angular/common": "5.0.0-beta.7",
    "@angular/compiler": "5.0.0-beta.7",
    "@angular/core": "5.0.0-beta.7",
    "@angular/forms": "5.0.0-beta.6",
    "@angular/http": "5.0.0-beta.7",
    "@angular/platform-browser": "5.0.0-beta.7",
    "@angular/platform-browser-dynamic": "5.0.0-beta.7",
    "@angular/router": "^5.0.0-beta.7",
    "blueprint-css": "^2.2.2",
    "core-js": "^2.4.1",
    "intl": "^1.2.5",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0-beta.1",
    "@angular/compiler-cli": "5.0.0-beta.7",
    "@angular/language-service": "5.0.0-beta.7",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "sass-lint": "1.10.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.3.2",
    "typescript": "2.5.2",
    "webpack-bundle-analyzer": "2.9.0",
    "workbox-build": "2.0.2-rc1"
  }
